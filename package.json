{
    "name": "react_gas_template",
    "version": "1.0",
    "description": "React GAS Template (Change this description)",
    "scripts": {
      "gin": "clasp login",
      "gout": "clasp logout",
      "gcreate": "clasp create --title Project_Monitor_React --rootDir ./src",
      "gstart": "clasp push --force --watch",
      "gpush": "clasp push --force",
      "clean": "del-cli ./.parcel-cache ./dist \"./src/*\" \"!./src/.claspignore\" \"!./src/appsscript.json\" \"!./src/main.js\"",
      "build": "parcel build optimize src_react/index.html --dist-dir ./src",
      "clean_build": "npm run clean && npm run build",
      "start": "parcel src_react/index.html --dist-dir ./src_gas"
    },
    "author": "MSIconsulting",
    "license": "ISC",
    "@parcel/resolver-default": {
      "packageExports": true
    },
    "devDependencies": {
      "@parcel/resolver-glob": "^2.12.0",
      "@parcel/transformer-sass": "^2.12.0",
      "@types/jquery": "^3.5.29",
      "buffer": "^6.0.3",
      "concurrently": "^8.2.2",
      "del-cli": "^5.1.0",
      "parcel": "^2.12.0",
      "process": "^0.11.10"
    },
    "dependencies": {
      "@reduxjs/toolkit": "^2.2.5",
      "axios": "^1.6.8",
      "body-parser": "^1.20.2",
      "bootstrap": "^5.3.2",
      "bootstrap-icons": "^1.11.3",
      "chart.js": "^4.4.2",
      "cors": "^2.8.5",
      "jquery": "^3.7.1",
      "nodemon": "^3.0.2",
      "react": "^18.3.1",
      "react-bootstrap": "^2.10.2",
      "react-bootstrap-icons": "^1.11.4",
      "react-chartjs-2": "^5.2.0",
      "react-dnd": "^16.0.1",
      "react-dnd-html5-backend": "^16.0.1",
      "react-dom": "^18.3.1",
      "react-hot-toast": "^2.4.1",
      "react-redux": "^8.1.3",
      "react-router-dom": "^6.20.0",
      "redux": "^4.2.1"
    }
  }
  